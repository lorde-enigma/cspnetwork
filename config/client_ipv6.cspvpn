# csp network vpn client configuration
# similar to openvpn format

# server connection
remote 206.0.29.4 8080

# client identification
client-id ipv6-client-deploy
auth-token secret-ipv6-token-deploy-2025
client-seed 2025092567890
exit-ip 10.8.0.200

# connection settings
protocol udp
keepalive 30
connect-timeout 10
auto-reconnect true

# logging
log-level info

# seed-based routing (managed automatically)
# each client gets unique exit IP based on seed
